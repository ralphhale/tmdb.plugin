{
  "manifest_version": 3,
  "name": "IMDB FIle Info Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "imdb-info.html",
    "default_icon": "imdb_icon.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  }
}